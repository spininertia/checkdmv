{
  "manifest_version": 2,

  "name": "CheckDMV",
  "description": "This extension checks the appointment for driver's license test.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "images/icon.png"
    // "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["js/checkdmv.js", "js/jquery-1.11.1.min.js"]
  },
  "permissions": [
    "storage",
    "alarms",
    "tabs",
    "notifications",
    "https://www.dmv.ca.gov/"
  ]
  // "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
